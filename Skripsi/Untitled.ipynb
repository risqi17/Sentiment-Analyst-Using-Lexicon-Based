   "source": [
    "import re\n",
    "from nltk.util import ngrams\n",
    "s = \"apakah saya harus makan dengan ayam goreng atau dengan ayang kamu\"\n",
    "s = s.lower()\n",
    "s = re.sub(r'[^a-zA-Z0-9\\s]', ' ', s)\n",
    "tokens = [token for token in s.split(\" \") if token != \"\"]\n",
    "output = list(ngrams(tokens, 2))\n",
    "output"
   ]
